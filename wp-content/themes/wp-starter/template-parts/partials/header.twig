			<nav class="main-navigation grow lg:grow-0 lg:ml-10">
				<ul class="main-menu">
					{% if menu and menu.items %}
						{% for item in menu.items %}
							<li class="{{ item.classes | join(' ') }}">
								<a class="inline-flex items-center transition-all" href="{{ item.link }}" title="Go to {{ item.title }} page.">
									{{ item.title }}
									{% if item.children %}
										<svg class="ml-2 lg:hidden" width="24" height="15" viewbox="0 0 24 15" fill="none" xmlns="http://www.w3.org/2000/svg">
											<path d="M22.7617 1.73828L12.1805 13.0762C11.9492 13.3241 11.5736 13.3241 11.3422 13.0762L0.76172 1.73828" stroke="white" stroke-width="1.5" stroke-miterlimit="10" stroke-linecap="round"/>
										</svg>
										<svg class="hidden ml-2 lg:block" width="10" height="8" viewbox="0 0 10 8" fill="none" xmlns="http://www.w3.org/2000/svg">
											<path d="M9 3L5.1523 6.93546C5.06817 7.02151 4.93158 7.02151 4.84745 6.93546L1 3" stroke="white" stroke-width="1.2" stroke-miterlimit="10" stroke-linecap="square" stroke-linejoin="bevel"/>
										</svg>
									{% endif %}
								</a>
								{% if item.children %}
									<ul class="sub-menu">
										{% for child in item.children %}
											<li class="{{ child.classes | join(' ') }}">
												<a target="{{child.target}}" href="{{ child.link }}" title="Go to {{ child.title }} page.">{{ child.title }}</a>
											</li>
										{% endfor %}
									</ul>
								{% endif %}
							</li>
						{% endfor %}
					{% endif %}
				</ul>
			</nav>
