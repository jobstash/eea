{% set fees = fields.membership_fees %}
{% if fees %}
	<ul class="py-6 mt-4 space-y-4 md:grid md:grid-cols-2 md:space-y-0 md:gap-8 lg:grid-cols-3">
		{% for sub_item in fees %}
			<li class="p-5 space-y-6 bg-white/10">
				<h4 class="text-2xl font-bold leading-[150%] antialiased">{{ sub_item.title }}</h4>
				<p class="text-base wysiwyg text-white/80 leading-[200%]">{{ sub_item.text }}</p>
				<p class="text-lg leading-[150%] text-[#F9E7C2] tracking-tight">{{ sub_item.revenues }}/ year</p>
			</li>
		{% endfor %}
	</ul>
{% endif %}
